<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.6 0.4 0.2"/>
            </geometry>
            <material name="blue"/>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.6 0.4 0.2"/>
            </geometry>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
        </collision>
        <xacro:box_inertia m="5.0" l="0.6" w="0.4" h="0.2" xyz="0 0 0.1" rpy="0 0 0" />
    </link>

    <link name="camera">
        <visual>
            <geometry>
                <box size="0.02 0.01 0.03" />
            </geometry>
            <material name="red" />
            <origin xyz="0 0 0" rpy="0 0 0" />
        </visual>
        <collision>
            <geometry>
                <box size="0.02 0.01 0.03" />
            </geometry>
            <material name="red" />
            <origin xyz="0 0 0" rpy="0 0 0" />
        </collision>
        <xacro:box_inertia m="0.5" l="0.02" w="0.01" h="0.03" xyz="0.3 0.2 0.1" rpy="0 0 0" />
    </link>

    <joint name="joint_cam" type="fixed">
        <parent link="base_link" />
        <child link="camera" />
        <origin xyz="0.3 0 0.1" rpy="0 0 0" />
    </joint>
    

    <link name="lidar_link">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.1" />
            </geometry>
            <material name="white" />
            <origin xyz="0.1 0 0.05" rpy="0 0 0" />
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.1" />
            </geometry>
            <origin xyz="0.1 0 0.05" rpy="0 0 0" />
        </collision>
        <xacro:cylinder_inertia m="0.5" r="0.05" h="0.1" xyz="0.1 0 0.05" rpy="0 0 0" />
    </link>
    

    <joint name="joint_1" type="fixed">
        <parent link="base_link" />
        <child link="lidar_link" />
        <origin xyz="0 0 0.2" rpy="0 0 0" />
    </joint>

    <link name="wheel_right_1">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <material name="white" />
            <origin xyz="0 0 0.025" rpy="0 0 0" />
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <origin xyz="0 0 0.025" rpy="0 0 0" />
        </collision>
        <xacro:cylinder_inertia m="1.0" r="0.1" h="0.05" xyz="0 0 0.025" rpy="0 0 0" />
    </link>
    

    <joint name="joint_2" type="continuous">
        <parent link="base_link" />
        <child link="wheel_right_1" />
        <axis xyz="0 0 1" />
        <origin xyz="0.2 -0.2 0.05" rpy="1.57 0 0" />
        
    </joint>

    <link name="wheel_right_2">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <material name="white" />
            <origin xyz="0 0 0.025" rpy="0 0 0" />
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <origin xyz="0 0 0.025" rpy="0 0 0" />
        </collision>
        <xacro:cylinder_inertia m="1.0" r="0.1" h="0.05" xyz="0 0 0.025" rpy="0 0 0" />
    </link>
    

    <joint name="joint_3" type="continuous">
        <parent link="base_link" />
        <child link="wheel_right_2" />
        <axis xyz="0 0 1" />
        <origin xyz="-0.2 -0.2 0.05" rpy="1.57 0 0" />
    </joint>

    <link name="wheel_left_1">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <material name="white" />
            <origin xyz="0 0 -0.025" rpy="0 0 0" />
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <origin xyz="0 0 -0.025" rpy="0 0 0" />
        </collision>
        <xacro:cylinder_inertia m="1.0" r="0.1" h="0.05" xyz="0 0 -0.025" rpy="0 0 0" />
    </link>
    

    <joint name="joint_4" type="continuous">
        <parent link="base_link" />
        <child link="wheel_left_1" />
        <axis xyz="0 0 1" />
        <origin xyz="0.2 0.2 0.05" rpy="1.57 0 0" />
        
    </joint>

    <link name="wheel_left_2">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <material name="white" />
            <origin xyz="0 0 -0.025" rpy="0 0 0" />
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <origin xyz="0 0 -0.025" rpy="0 0 0" />
        </collision>
        <xacro:cylinder_inertia m="1.0" r="0.1" h="0.05" xyz="0 0 -0.025" rpy="0 0 0" />
    </link>
    

    <joint name="joint_5" type="continuous">
        <parent link="base_link" />
        <child link="wheel_left_2" />
        <axis xyz="0 0 1" />
        <origin xyz="-0.2 0.2 0.05" rpy="1.57 0 0" />
    </joint>

    <link name="base_footprint" />
    <joint name="base_joint" type="fixed">
        <parent link="base_footprint" />
        <child link="base_link" />
        <origin xyz="0 0 0.05" rpy="0 0 0" />
    </joint>
</robot>